<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="simple.blog.app.entity.AdministratorEntity" table="administrator" schema="hostel">
        <id name="idAdministrator">
            <column name="id_administrator" sql-type="int(10) unsigned"/>
        </id>
        <property name="login">
            <column name="login" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(40)" length="40"/>
        </property>
    </class>
</hibernate-mapping>